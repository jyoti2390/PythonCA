# Creating Class Employee
class Employee
def __init__(self,StaffID, LastName, FirstName, RegHours, HourlyRate, OTMultiple, TaxCredit, StandardBand):
self.Id=staffId
self.fname=FirstName
self.lname=LastName
self.rhours=RegHours
self.hRate=HourlyRate
self.ot=OTMultiple
self.taxC=TaxCredit
self.sb=StandardBand
